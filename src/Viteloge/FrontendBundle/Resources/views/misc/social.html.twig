{% block misc_social %}
{% spaceless %}
    <div>
        {% trans %}If you like fasthousing dot com click on{% endtrans %}
        <div class="fb-like" data-href="{{ 'http://facebook.com/'~facebook_account }}" data-layout="button_count" data-action="like" data-show-faces="false" data-share="false"></div>
        {% trans %}Follow all Fasthousing news on{% endtrans %}
        <a href="{{ 'http://facebook.com/'~facebook_account }}" class="round-icon small facebook" target="_blank" aria-label="Facebook">
            <span class="fa fa-facebook" aria-hidden="true"></span>
        </a>
        <a href="{{ 'http://twitter.com/'~twitter_account }}" class="round-icon small twitter" target="_blank" aria-label="Twitter">
            <span class="fa fa-twitter" aria-hidden="true"></span>
        </a>
    </div>
{% endspaceless %}
{% endblock %}
