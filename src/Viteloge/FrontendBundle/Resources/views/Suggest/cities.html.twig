{#{% extends 'VitelogeFrontendBundle::base.html.twig' %}#}

{%- block main -%}
    {%- if cities -%}
    {"results":[{%- for key, city in cities -%}
            {"id":"{{ city.id }}","text":"{{ city.name ~ ' (' ~ city.postalcode ~ ')'}}"}{%- if not loop.last -%},{%- endif -%}
        {%- endfor -%}]}
    {%- else-%}
    {"results":[]}
    {%- endif -%}
{%- endblock -%}
