{%- if not ad.privilege.isExclu -%}
    {{ include('VitelogeCoreBundle:Global:sticker.html.twig', { title: 'Exclusivity'|trans }) }}
{%- endif -%}
<a href="{{ path('viteloge_frontend_ad_redirect', { 'id': ad.id }) }}" class="preview">
    <img itemprop="image" src="{{ 'http://img.viteloge.com/' ~ ad.photo }}" class="img-responsive" alt="">
</a>
{%- if ad.privilege.hasLogo -%}
<div class="pos-relative">
    <img itemprop="logo" src="{{ 'http://img.viteloge.com/logos/agence.' ~ ad.agencyId ~ '.gif' }}" class="agency-logo img-responsive" alt="logo">
</div>
{%- endif -%}
