{%- if ad.privilege.isExclu -%}
    {{ include('VitelogeCoreBundle:Global:sticker.html.twig', { title: 'ad.sticker.exlusivity'|trans }) }}
{%- endif -%}
<a href="{{ path('viteloge_frontend_ad_redirect', { 'id': ad.id }) }}" class="preview" target="_blank">
    <img itemprop="image" data-original="{{ aws_s3_domain(ad.photo) }}" class="lazy img-responsive" alt="">
    <noscript><img itemprop="image" src="{{ aws_s3_domain(ad.photo) }}" class="img-responsive" alt=""></noscript>
</a>
{%- if ad.privilege.hasLogo -%}
<div class="pos-relative">
    <img itemprop="logo" src="{{ aws_s3_domain('logos/agence.'~ad.agencyId~'.gif') }}" class="agency-logo img-responsive" alt="logo">
</div>
{%- endif -%}
