<!doctype html>
<html {{ sonata_seo_html_attributes() }}>{#<!--manifest="viteloge.appcache"-->#}
    <head {{ sonata_seo_head_attributes() }}>
        {%- block head_meta -%}
            {{ sonata_seo_metadatas() }}
            {{ sonata_seo_link_canonical() }}
            {{ sonata_seo_lang_alternates() }}
        {%- endblock head_meta -%}
        {%- block title -%}
            {{ sonata_seo_title() }}
        {%- endblock title -%}
        {%- block head_icon -%}
            <link rel="apple-touch-startup-image" href="{{ aws_s3_domain('bundles/vitelogecore/images/logo-viteloge.png') }}">
            <link rel="apple-touch-icon" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon.png') }}">
            <link rel="apple-touch-icon" sizes="57x57" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-57x57.png') }}">
            <link rel="apple-touch-icon" sizes="60x60" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-60x60.png') }}">
            <link rel="apple-touch-icon" sizes="72x72" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-72x72.png') }}">
            <link rel="apple-touch-icon" sizes="76x76" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-76x76.png') }}">
            <link rel="apple-touch-icon" sizes="114x114" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-114x114.png') }}">
            <link rel="apple-touch-icon" sizes="120x120" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-120x120.png') }}">
            <link rel="apple-touch-icon" sizes="144x144" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-144x144.png') }}">
            <link rel="apple-touch-icon" sizes="152x152" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-152x152.png') }}">
            <link rel="apple-touch-icon" sizes="180x180" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/apple/apple-touch-icon-180x180.png') }}">
            <link rel="icon" type="image/png" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/favicon-16x16.png') }}" sizes="16x16">
            <link rel="icon" type="image/png" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/favicon-32x32.png') }}" sizes="32x32">
            <link rel="icon" type="image/png" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/favicon-194x194.png') }}" sizes="194x194">
            <link rel="icon" type="image/png" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/favicon-96x96.png') }}" sizes="96x96">
            <link rel="icon" type="image/png" href="{{ aws_s3_domain('bundles/vitelogecore/images/favicon/android/android-chrome-192x192.png') }}" sizes="192x192">
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}">
            <link rel="author" type="text/plain" href="humans.txt">
        {%- endblock head_icon -%}
        {%- block head_stylesheets -%}
            <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
            {#<link rel="stylesheet" href="{{ aws_s3_domain('built/viteloge.min.css') }}">#}
            {% if app.environment == 'dev' %}
                <link rel="stylesheet" href="{{ aws_s3_file('assets/css/font-awesome.css', false) }}">
                <link rel="stylesheet" href="{{ aws_s3_file('assets/css/normalize.css', false) }}">
                <link rel="stylesheet" href="{{ aws_s3_file('assets/css/owl.carousel.css', false) }}">
                <link rel="stylesheet" href="{{ aws_s3_file('assets/css/select2.css', false) }}">
                <link rel="stylesheet" href="{{ aws_s3_file('bundles/vitelogecore/css/viteloge-core.css', false) }}">
                <link rel="stylesheet" href="{{ aws_s3_file('built/css/frontend.css', false) }}">
                <link rel="stylesheet" href="{{ aws_s3_file('built/css/estimation.css', false) }}">
                <link rel="stylesheet" href="{{ aws_s3_file('built/css/select2/theme/viteloge/layout.css', false) }}">
            {% else %}
                <link rel="stylesheet" href="{{ aws_s3_file('built/viteloge.min.css') }}">
            {% endif %}
        {%- endblock head_stylesheets -%}
        {%- block head_javascripts -%}
            {% if app.environment == 'dev' %}
                <script src="{{ 'http://dev.viteloge.com/built/head.js' }}"></script>
            {% else %}
                <script src="{{ aws_s3_file('built/head.min.js') }}"></script>
            {% endif %}
            <script src="http://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
        {% block head_javascripts_requirejs %}
        {% endblock head_javascripts_requirejs %}
        {%- endblock head_javascripts -%}
    </head>

    <body class="body theme-default {% if app.request.attributes.get('_route') == 'viteloge_frontend_homepage' %}home{% endif %}" {% block schema %}itemscope itemtype="http://schema.org/WebPage"{% endblock schema %} {% if app.request.attributes.get('_route') == 'viteloge_frontend_agency_view' %}style="overflow-y: hidden;" {% endif %}>
    {%- block init_javascripts -%}
        <script>
          window.fbAsyncInit = function() {
            FB.init({
              appId : '{{ facebook_client_id }}',
              cookie : true,
              xfbml : true,
              version : '{{ facebook_app_version }}'
            });
          };

          (function(d, s, id){
             var js, fjs = d.getElementsByTagName(s)[0];
             if (d.getElementById(id)) {return;}
             js = d.createElement(s); js.id = id;
             js.src = "https://connect.facebook.net/fr_FR/sdk.js";
             fjs.parentNode.insertBefore(js, fjs);
           }(document, 'script', 'facebook-jssdk'));
        </script>
        <script>
        window.twttr = (function(d, s, id) {
          var js, fjs = d.getElementsByTagName(s)[0],
            t = window.twttr || {};
          if (d.getElementById(id)) return t;
          js = d.createElement(s);
          js.id = id;
          js.src = "https://platform.twitter.com/widgets.js";
          fjs.parentNode.insertBefore(js, fjs);

          t._e = [];
          t.ready = function(f) {
            t._e.push(f);
          };

          return t;
        }(document, "script", "twitter-wjs"));
        </script>
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

          ga('create', '{{ ga_tracking }}', 'auto');
          ga('send', 'pageview');
        </script>
    {%- endblock init_javascripts -%}
    {%- block html_start -%}

    {%- endblock html_start -%}
    {%- block body -%}
        <header class="header" data-role="header" {% if app.request.attributes.get('_route') == 'viteloge_frontend_agency_view' or app.request.attributes.get('_route') == 'viteloge_frontend_favourite_view' %}style="border-bottom: 3px solid #00a1c6;"{% endif %}>
            {%- block header_start %}

            {% endblock -%}
            {%- block header -%}
                {{ include('VitelogeUserBundle:base:header.html.twig') }}
            {%- endblock header -%}
            <div class="subheader {% if app.request.attributes.get('_route') != 'viteloge_frontend_agency_view' and app.request.attributes.get('_route') != 'viteloge_frontend_favourite_view'  %}subheader-paginate{% endif %}">
                {%- block breadcrumb %}
                    {{ wo_render_breadcrumbs() }}
                {% endblock breadcrumb -%}
                {%- block header_end %}

                {% endblock -%}
            </div>
            {%- block header_more %}

            {% endblock header_more -%}
        </header>
        <main class="main clearfix{% if app.request.attributes.get('_route') != 'viteloge_frontend_homepage' %}{% if app.request.attributes.get('_route') == 'viteloge_frontend_ad_search' or app.request.attributes.get('_route') == 'viteloge_frontend_ad_latest_list' or app.request.attributes.get('_route') == 'viteloge_frontend_favourite_list' %} main-top {% elseif app.request.attributes.get('_route') == 'viteloge_frontend_ad_view' %} main-site {% elseif app.request.attributes.get('_route') == 'viteloge_frontend_agency_view' or app.request.attributes.get('_route') == 'viteloge_frontend_favourite_view' %} main-search {% else %} main-small {% endif %}{% endif %}" role="main">

        {%- block body_start -%}

        {%- endblock body_start -%}
        {%- block main -%}

        {%- endblock main -%}
        {%- block body_end -%}

        {%- endblock body_end -%}
        </main>
        <footer data-role="clearfix footer">
        {%- block footer_start -%}

        {%- endblock -%}
        {%- block footer -%}
            <div class="container text-center">
                {{- include('VitelogeFrontendBundle:menu:footer.html.twig', {}) -}}
                {{- include('VitelogeFrontendBundle:misc:social.html.twig', {}) -}}
            </div>
            <div class="container text-center text-secondary">
                {{- include('VitelogeFrontendBundle:misc:copyright.html.twig', {}) -}}
                {{- include('VitelogeFrontendBundle:Block:sites.html.twig', {}) -}}
            </div>
        {%- endblock footer -%}
        </footer>
    {%- endblock body -%}
    {%- block html_end -%}
        {{- include('VitelogeFrontendBundle:misc:cookie-policy.html.twig', {}) -}}
        {{- include('VitelogeFrontendBundle:misc:loader.html.twig', {}) -}}
        {{- include('VitelogeFrontendBundle:misc:backtotop.html.twig', {}) -}}
    {%- endblock html_end -%}
    {%- block javascripts %}
        {% if app.environment == 'dev' %}
            <script src="{{ aws_s3_file('built/global.js', false) }}"></script>
        {% else %}
            <script src="{{ aws_s3_file('built/global.min.js') }}"></script>
        {% endif %}
        <script src="{{ url('bazinga_jstranslation_js') }}"></script>
    {% endblock javascripts -%}
    </body>
</html>
