<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Viteloge\CoreBundle\Entity\Barometer" table="barometre" repository-class="Viteloge\CoreBundle\Repository\BarometerRepository">
    <indexes>
      <index name="state" columns="region"/>
      <index name="department" columns="departement"/>
      <index name="itt" columns="insee,transaction,type"/>
    </indexes>
    <id name="year" type="smallint" column="annee"/>
    <id name="month" type="smallint" column="mois"/>
    <id name="type" type="string" column="type" length="1"/>
    <!--<id name="cityId" type="string" column="insee" length="5"/>-->
    <many-to-one field="inseeCity" target-entity="Acreat\InseeBundle\Entity\InseeCity">
      <join-columns>
        <join-column name="insee" referenced-column-name="codeInsee"/>
      </join-columns>
    </many-to-one>
    <id name="transaction" type="string" column="transaction" length="1"/>
    <!--<field name="stateId" type="smallint" column="region" nullable="false"/>-->
    <many-to-one field="inseeState" target-entity="Acreat\InseeBundle\Entity\InseeState">
      <join-columns>
        <join-column name="region" referenced-column-name="REGION"/>
      </join-columns>
    </many-to-one>
    <!--<field name="departmentId" type="string" column="departement" length="3" nullable="false"/>-->
    <many-to-one field="inseeDepartment" target-entity="Acreat\InseeBundle\Entity\InseeDepartment">
      <join-columns>
        <join-column name="departement" referenced-column-name="DEP"/>
      </join-columns>
    </many-to-one>
    <field name="total" type="integer" column="nb" nullable="false"/>
    <field name="avgSqm" type="float" column="avg_sqm" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
