<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Viteloge\CoreBundle\Entity\Ad" table="export_annonce" repository-class="Viteloge\CoreBundle\Repository\AdRepository">
    <indexes>
      <index name="cityId" columns="codeInsee"/>
      <index name="agencyId" columns="idAgence"/>
      <index name="transaction" columns="transaction"/>
      <index name="rooms" columns="nbpiece"/>
      <index name="price" columns="prix"/>
      <index name="districtId" columns="arrondissement"/>
      <index name="agencySpecial" columns="specifAgence"/>
      <index name="privilegeId" columns="idPrivilege"/>
      <index name="privilegeCode" columns="codePrivilege"/>
      <index name="privilegeRank" columns="rankPrivilege"/>
      <index name="description" columns="description"/>
      <index name="order" columns="ordre"/>
      <index name="createdAt" columns="dateInsert"/>
      <index name="updatedAt" columns="dateUpdate"/>
    </indexes>
    <id name="id" type="integer" column="idAnnonce">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="agencyId" type="integer" column="idAgence" nullable="false"/>
    <field name="agencyName" type="string" column="agence" length="255" nullable="false"/>
    <field name="agencySpecial" type="string" column="specifAgence" length="255" nullable="false"/>
    <field name="url" type="string" column="url" length="255" nullable="false"/>
    <field name="transaction" type="string" column="transaction" length="1" nullable="false"/>
    <field name="type" type="string" column="type" length="50" nullable="false"/>
    <field name="rooms" type="integer" column="nbpiece" nullable="false"/>
    <field name="surface" type="integer" column="Surface" nullable="true"/>
    <field name="price" type="float" column="prix" precision="10" scale="0" nullable="false"/>
    <!--<field name="cityId" type="string" column="codeInsee" length="5" nullable="false"/>-->
    <many-to-one field="inseeCity" target-entity="Acreat\InseeBundle\Entity\InseeCity">
      <join-columns>
        <join-column name="codeInsee" referenced-column-name="codeInsee"/>
      </join-columns>
    </many-to-one>
    <field name="cityName" type="string" column="commune" length="150" nullable="false"/>
    <field name="districtId" type="smallint" column="arrondissement" nullable="false"/>
    <field name="postalCode" type="string" column="codepostal" length="8" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="descriptionMku" type="text" column="description_mku" nullable="true"/>
    <field name="photo" type="string" column="photo" length="200" nullable="false"/>
    <field name="order" type="float" column="ordre" precision="10" scale="0" nullable="false"/>
    <field name="createdAt" type="datetime" column="dateInsert" nullable="false"/>
    <field name="updatedAt" type="datetime" column="dateUpdate" nullable="false"/>
    <field name="publicationLimit" type="smallint" column="limitPublication" nullable="false"/>
    <field name="privilegeId" type="integer" column="idPrivilege" nullable="false"/>
    <field name="privilegeCode" type="string" column="codePrivilege" length="10" nullable="false"/>
    <field name="privilegeRank" type="smallint" column="rankPrivilege" nullable="false"/>
  </entity>
</doctrine-mapping>
