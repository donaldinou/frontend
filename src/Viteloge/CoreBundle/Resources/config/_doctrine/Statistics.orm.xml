<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Viteloge\CoreBundle\Entity\Statistics" table="statistiques" repository-class="Viteloge\CoreBundle\Repository\StatisticsRepository">
    <indexes>
      <index name="date" columns="date"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <field name="year" type="smallint" column="annee" nullable="false"/>
    <field name="month" type="smallint" column="mois" nullable="false"/>
    <field name="day" type="smallint" column="jour" nullable="false"/>
    <field name="ip" type="string" column="ip" length="15" nullable="false"/>
    <field name="ua" type="string" column="UA" length="128" nullable="false"/>
    <!--<field name="adId" type="integer" column="idAnnonce" nullable="false"/>-->
    <many-to-one field="ad" target-entity="Ad">
      <join-columns>
        <join-column name="idAnnonce" referenced-column-name="idAnnonce"/>
      </join-columns>
    </many-to-one>
    <field name="agencyId" type="integer" column="idAgence" nullable="false"/>
    <field name="agencyName" type="string" column="agence" length="100" nullable="false"/>
    <field name="agencySpecial" type="string" column="specifAgence" length="255" nullable="false"/>
    <field name="url" type="string" column="url" length="255" nullable="false"/>
    <field name="transaction" type="string" column="transaction" length="1" nullable="false"/>
    <field name="type" type="string" column="type" length="50" nullable="false"/>
    <field name="rooms" type="boolean" column="nbpiece" nullable="false"/>
    <field name="price" type="float" column="prix" precision="10" scale="0" nullable="false"/>
    <field name="cityName" type="string" column="commune" length="255" nullable="false"/>
    <field name="distictId" type="smallint" column="arrondissement" nullable="false"/>
    <!--<field name="cityId" type="integer" column="codeInsee" nullable="false"/>-->
    <many-to-one field="inseeCity" target-entity="Acreat\InseeBundle\Entity\InseeCity">
      <join-columns>
        <join-column name="codeInsee" referenced-column-name="codeInsee"/>
      </join-columns>
    </many-to-one>
    <field name="postalcode" type="string" column="codepostal" length="8" nullable="false"/>
  </entity>
</doctrine-mapping>
