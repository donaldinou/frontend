<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Viteloge\CoreBundle\Entity\Sponsor" table="sponsor" repository-class="Viteloge\CoreBundle\Repository\SponsorRepository">
    <id name="id" type="integer" column="idSponsor">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="typeSponsor" length="20" nullable="false"/>
    <!--<field name="cityId" type="string" column="inseeSponsor" length="255" nullable="false"/>-->
    <many-to-one field="inseeCity" target-entity="InseeCity">
      <join-columns>
        <join-column name="inseeSponsor" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <field name="transaction" type="enumtransaction" column="transacSponsor" nullable="false"/>
    <field name="title" type="string" column="titreSponsor" length="50" nullable="false"/>
    <field name="data" type="string" column="texteSponsor" length="100" nullable="false"/>
    <field name="url" type="string" column="urlSponsor" length="255" nullable="false"/>
    <field name="link" type="string" column="linkSponsor" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
