<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Viteloge\CoreBundle\Entity\Distance" table="distancier" repository-class="Viteloge\CoreBundle\Repository\DistanceRepository">
    <indexes>
      <index name="distance" columns="distance"/>
      <index name="cityIdFrom" columns="codeInseeFrom"/>
      <index name="cityIdTo" columns="codeInseeTo"/>
    </indexes>
    <!--<id name="cityIdFrom" type="string" column="codeInseeFrom" length="5"/>-->
    <many-to-one field="inseeFrom" target-entity="InseeCity">
      <join-columns>
        <join-column name="codeInseeFrom" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <!--<id name="cityIdTo" type="string" column="codeInseeTo" length="5"/>-->
    <many-to-one field="inseeTo" target-entity="InseeCity">
      <join-columns>
        <join-column name="codeInseeTo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <field name="distance" type="float" column="distance" precision="10" scale="0" nullable="false"/>
    <field name="googleDistance" type="float" column="google_distance" precision="10" scale="0" nullable="false"/>
    <field name="googleTime" type="time" column="google_time" nullable="false"/>
  </entity>
</doctrine-mapping>
