<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acreat\InseeBundle\Entity\InseeArea" table="insee_quartiers" repository-class="Acreat\InseeBundle\Repository\InseeAreaRepository">
    <indexes>
      <index name="cityId" columns="insee"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <!--<field name="cityId" type="string" column="insee" length="5" nullable="false"/>-->
    <many-to-one field="inseeCity" target-entity="InseeCity">
      <join-columns>
        <join-column name="insee" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="keywords" type="text" column="keywords" nullable="false"/>
    <field name="polyline" type="text" column="polyline" nullable="false"/>
    <field name="levels" type="string" column="levels" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
