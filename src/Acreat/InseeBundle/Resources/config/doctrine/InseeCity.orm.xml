<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acreat\InseeBundle\Entity\InseeCity" table="insee_communes" repository-class="Acreat\InseeBundle\Repository\InseeCityRepository">
    <indexes>
      <index name="postalcode" columns="cp"/>
      <index name="lat" columns="lat"/>
      <index name="lng" columns="lon"/>
      <index name="geolevel" columns="niveauGeo"/>
      <index name="isCapital" columns="chefLieu"/>
      <index name="stateId" columns="codeRegion"/>
      <index name="departmentId" columns="codeDepartement"/>
      <index name="code" columns="codeCommune"/>
      <index name="distictId" columns="codeArrondissment"/>
      <index name="cantonId" columns="codeCanton"/>
      <index name="population" columns="population"/>
      <index name="basin" columns="bassinVie"/>
      <index name="uname" columns="search_nom"/>
      <index name="name" columns="nom"/>
    </indexes>
    <id name="id" type="string" column="codeInsee" length="5">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="geolevel" type="string" column="niveauGeo" length="3" nullable="false"/>
    <field name="isCantonDivided" type="smallint" column="decoupageCanton" nullable="false"/>
    <field name="isCapital" type="smallint" column="chefLieu" nullable="false"/>
    <!--<field name="stateId" type="string" column="codeRegion" length="2" nullable="false"/>-->
    <many-to-one field="inseeState" target-entity="InseeState">
      <join-columns>
        <join-column name="codeRegion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <!--<field name="departmentId" type="string" column="codeDepartement" length="3" nullable="false"/>-->
    <many-to-one field="inseeDepartment" target-entity="InseeDepartment">
      <join-columns>
        <join-column name="codeDepartement" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <field name="code" type="string" column="codeCommune" length="3" nullable="false"/>
    <field name="districtId" type="boolean" column="codeArrondissment" nullable="false"/>
    <field name="cantonId" type="string" column="codeCanton" length="2" nullable="false"/>
    <field name="prefixId" type="smallint" column="typeNomClair" nullable="false"/>
    <field name="prefix" type="string" column="article" length="10" nullable="false"/>
    <field name="uname" type="text" column="search_nom" nullable="false"/>
    <field name="name" type="text" column="nom" nullable="false"/>
    <field name="lat" type="float" column="lat" precision="10" scale="0" nullable="false"/>
    <field name="lng" type="float" column="lon" precision="10" scale="0" nullable="false"/>
    <field name="postalcode" type="string" column="cp" length="8" nullable="false"/>
    <field name="basin" type="string" column="bassinVie" length="5" nullable="false"/>
    <field name="population" type="integer" column="population" nullable="false"/>
  </entity>
</doctrine-mapping>
