<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acreat\InseeBundle\Entity\InseeCityhall" table="insee_mairies" repository-class="Acreat\InseeBundle\Repository\InseeCityhallRepository">
    <indexes>
      <index name="population" columns="population"/>
    </indexes>
    <id name="cityId" type="string" column="codeInsee" length="5">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="address" type="string" column="adresse" length="255" nullable="false"/>
    <field name="postalcode" type="string" column="cp" length="5" nullable="false"/>
    <field name="city" type="string" column="ville" length="255" nullable="false"/>
    <field name="phone" type="string" column="tel" length="50" nullable="false"/>
    <field name="fax" type="string" column="fax" length="50" nullable="false"/>
    <field name="mail" type="string" column="mail" length="255" nullable="false"/>
    <field name="url" type="string" column="url" length="255" nullable="false"/>
    <field name="mayor" type="string" column="maire" length="255" nullable="false"/>
    <field name="population" type="integer" column="population" nullable="false"/>
  </entity>
</doctrine-mapping>
