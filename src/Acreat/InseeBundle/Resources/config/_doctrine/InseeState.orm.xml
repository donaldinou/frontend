<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acreat\InseeBundle\Entity\InseeState" table="insee_regions" repository-class="Acreat\InseeBundle\Repository\InseeStateRepository">
    <indexes>
      <index name="cityId" columns="CHEFLIEU"/>
    </indexes>
    <id name="id" type="string" column="REGION" length="2">
      <generator strategy="IDENTITY"/>
    </id>
    <!--<field name="cityId" type="string" column="CHEFLIEU" length="5" nullable="true"/>-->
    <many-to-one field="inseeCapital" target-entity="InseeCity">
      <join-columns>
        <join-column name="CHEFLIEU" referenced-column-name="codeInsee"/>
      </join-columns>
    </many-to-one>
    <field name="prefix" type="string" column="TNCC" length="1" nullable="true"/>
    <field name="uname" type="string" column="NCC" length="70" nullable="true"/>
    <field name="name" type="string" column="NCCENR" length="70" nullable="true"/>
  </entity>
</doctrine-mapping>
